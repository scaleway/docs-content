{
  "ignorePatterns": [
    {
      "pattern": "^http://scaleway.com"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/.attachments",
      "replacement": "https://scaleway.com/en/docs/.attachments"
    },
    {
      "pattern": "^/",
      "replacement": "https://scaleway.com/en/docs/"
    },
    {
      "pattern": "%20",
      "replacement": "-",
      "global": true
    }
  ],
  "httpHeaders": [
    {
      "urls": [
        "https://scaleway.com/en/docs"
      ],
      "headers": {
        "Authorization": "Basic Zm9vOmJhcg==",
        "Foo": "Bar"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [
    200,
    206
  ]
}